{
  "name": "chrome-extension-boilerplate-react-vite",
  "version": "0.0.24",
  "description": "chrome extension boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/rajatdhoot123/chrome-extension-boilerplate.git"
  },
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:firefox": "tsc --noEmit && cross-env __FIREFOX__=true vite build",
    "build:watch": "cross-env __DEV__=true vite build -w --mode development",
    "build:firefox:watch": "cross-env __DEV__=true __FIREFOX__=true vite build -w --mode development",
    "build:hmr": "rollup --config utils/reload/rollup.config.mjs",
    "wss": "node utils/reload/initReloadServer.js",
    "dev": "pnpm build:hmr && (run-p wss build:watch)",
    "dev:firefox": "pnpm build:hmr && (run-p wss build:firefox:watch)",
    "test": "vitest",
    "commitlint": "commitlint --edit",
    "lint": "eslint src --ext .ts,.js,.tsx,.jsx",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --write",
    "prepare": "husky install"
  },
  "type": "module",
  "dependencies": {
    "@langchain/community": "^0.2.2",
    "@langchain/openai": "^0.0.29",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/supabase-js": "^2.42.3",
    "add": "^2.0.6",
    "button": "^1.1.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "construct-style-sheets-polyfill": "3.1.0",
    "input": "^1.0.1",
    "label": "^0.2.2",
    "lucide-react": "^0.379.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "^2.4.1",
    "select": "^1.1.2",
    "shadcn": "^2.3.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "textarea": "^0.3.0",
    "webextension-polyfill": "0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.1.0",
    "@rollup/plugin-sucrase": "^5.0.2",
    "@rollup/plugin-typescript": "11.1.6",
    "@testing-library/react": "14.0.0",
    "@types/chrome": "0.0.263",
    "@types/node": "20.8.10",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.18",
    "@types/ws": "8.5.8",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.18.1",
    "@vitejs/plugin-react": "4.2.0",
    "autoprefixer": "^10.4.19",
    "chokidar": "3.5.3",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.2",
    "fs-extra": "11.1.1",
    "husky": "8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "15.2.0",
    "magic-string": "^0.30.6",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.38",
    "prettier": "3.2.5",
    "rollup": "4.3.0",
    "sass": "1.72.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "ts-loader": "9.5.0",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "5.0.12",
    "vitest": "^1.6.0",
    "ws": "8.14.2"
  },
  "packageManager": "pnpm@9.0.4",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
